    d:DesignWidth="8400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="46"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="500"></ColumnDefinition>
            <ColumnDefinition Width="20"></ColumnDefinition>
            <ColumnDefinition Width="600"></ColumnDefinition>
            <ColumnDefinition Width="20"></ColumnDefinition>
            <ColumnDefinition Width="400"></ColumnDefinition>
            <ColumnDefinition Width="20"></ColumnDefinition>
            <ColumnDefinition Width="460"></ColumnDefinition>
            <ColumnDefinition Width="20"></ColumnDefinition>
            <ColumnDefinition Width="970"></ColumnDefinition>
            <ColumnDefinition Width="20"></ColumnDefinition>
            <ColumnDefinition Width="800"></ColumnDefinition>
            <ColumnDefinition Width="80"></ColumnDefinition>
            <ColumnDefinition Width="830"></ColumnDefinition>
            <ColumnDefinition Width="20"></ColumnDefinition>
            <ColumnDefinition Width="830"></ColumnDefinition>
            <ColumnDefinition Width="20"></ColumnDefinition>
            <ColumnDefinition Width="830"></ColumnDefinition>
            <ColumnDefinition Width="80"></ColumnDefinition>
            <ColumnDefinition Width="830"></ColumnDefinition>
            <ColumnDefinition Width="80"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid Grid.Row="0" Grid.Column="0">
            <StackPanel VerticalAlignment="Top">
                <TextBlock TextWrapping="Wrap" FontSize="26"  Text="Project Infomation" Foreground="#fff" FontFamily="Segoe UI light" Margin="5,5,5,5"/>
            </StackPanel>
        </Grid>
        <Grid Grid.Row="1" Grid.Column="0">
            <Grid.Background>
                <SolidColorBrush Color="Black" Opacity="0.5" />
            </Grid.Background>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="160"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>
            <TextBlock Grid.Row="0" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="System Number" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <ComboBox x:Name="cboSystemNo" Grid.Row="0" Grid.Column="1" Margin="5,5,5,5" Width="330" HorizontalAlignment="Left"/>
            <StackPanel Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Background="#000" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                <TextBlock FontSize="15" Foreground="#92d050" FontFamily="Segoe UI Regular" Margin="5,5,5,5" Text="TEST REQUIRMENTS" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </StackPanel>
            <TextBlock Grid.Row="2" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="DESIGN CODE" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <Grid Grid.Row="2" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="80"/>
                    <ColumnDefinition Width="80"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <CheckBox Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkB313" FlowDirection="RightToLeft">B31.3</CheckBox>
                <CheckBox Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkB311" FlowDirection="RightToLeft">B31.3</CheckBox>
            </Grid>
            <Grid Grid.Row="3" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="85"></ColumnDefinition>
                    <ColumnDefinition Width="57"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <CheckBox Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkDCOther" FlowDirection="RightToLeft">OTHER</CheckBox>
                <TextBlock Grid.Column="1" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="(Specify)" FontFamily="Segoe UI Regular" Margin="0,5,0,5"></TextBlock>
                <TextBox Grid.Column="2" x:Name="txtDCSpecify" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Margin="5,5,5,5" IsEnabled="False" IsReadOnly="True"/>
            </Grid>
            <TextBlock Grid.Row="4" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="TEST PACK NO" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBox Grid.Row="4" Grid.Column="1" x:Name="txtTPNo" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <Grid Grid.Row="5" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="80"></ColumnDefinition>
                    <ColumnDefinition Width="80"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <CheckBox Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkField" FlowDirection="RightToLeft">FIELD</CheckBox>
                <CheckBox Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkChop" FlowDirection="RightToLeft">SHOP</CheckBox>
            </Grid>
            <TextBlock Grid.Row="6" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="TEST TYPE" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <Grid Grid.Row="6" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="90"></ColumnDefinition>
                    <ColumnDefinition Width="100"></ColumnDefinition>
                    <ColumnDefinition Width="130"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <CheckBox Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkVisual" FlowDirection="RightToLeft">VISUAL</CheckBox>
                <CheckBox Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkService" FlowDirection="RightToLeft">SERVICE</CheckBox>
                <CheckBox Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkHydrostatic" FlowDirection="RightToLeft">HYDROSTATIC</CheckBox>
            </Grid>
            <TextBlock Grid.Row="7" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="SPEC TEST PRESSURE" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBlock Grid.Row="7" Grid.Column="1" x:Name="txtSTP" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBlock Grid.Row="8" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="SPEC TEST DIRATION" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBlock Grid.Row="8" Grid.Column="1" x:Name="txtSTD" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBlock Grid.Row="9" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" Text="TEST MEDIUM" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <Grid Grid.Row="9" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="90"/>
                    <ColumnDefinition Width="100"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <CheckBox Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkWater" FlowDirection="RightToLeft">WATER</CheckBox>
                <CheckBox Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkGlycol" FlowDirection="RightToLeft">GLYCOL</CheckBox>
            </Grid>
            <Grid Grid.Row="10" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="85"/>
                    <ColumnDefinition Width="57"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <CheckBox Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkTMOther" FlowDirection="RightToLeft">OTHER</CheckBox>
                <TextBlock Grid.Column="1" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="(Specify)" FontFamily="Segoe UI Regular" Margin="0,5,0,5"></TextBlock>
                <TextBox Grid.Column="2" x:Name="txtTMSpecify" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Margin="5,5,5,5" IsEnabled="False" IsReadOnly="True"/>
            </Grid>
        </Grid>
        <Grid Grid.Row="1" Grid.Column="2">
            <Grid.Background>
                <SolidColorBrush Color="Black" Opacity="0.5" />
            </Grid.Background>
            <ListView x:Name="lvLineNumberList" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                    SelectionMode="Single" 
                    ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                    ScrollViewer.VerticalScrollMode="Auto"
                    ScrollViewer.VerticalScrollBarVisibility="Auto">
                <ListView.Header>
                    <Grid HorizontalAlignment="Stretch" Height="40">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="140"></ColumnDefinition>
                            <ColumnDefinition Width="90"></ColumnDefinition>
                            <ColumnDefinition Width="90"></ColumnDefinition>
                            <ColumnDefinition Width="140"></ColumnDefinition>
                            <ColumnDefinition Width="140"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Center" Text="LINE NUMBER" FontFamily="Segoe UI Regular" Margin="5"></TextBlock>
                        <TextBlock Grid.Column="1" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Center" Text="LINE CLASS" FontFamily="Segoe UI Regular" Margin="5"></TextBlock>
                        <TextBlock Grid.Column="2" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Center" Text="P&amp;ID NO." FontFamily="Segoe UI Regular" Margin="5"></TextBlock>
                        <TextBlock Grid.Column="3" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Center" Text="ORIGIN" FontFamily="Segoe UI Regular" Margin="5"></TextBlock>
                        <TextBlock Grid.Column="4" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Center" Text="TERMINATION" FontFamily="Segoe UI Regular" Margin="5"></TextBlock>
                    </Grid>
                </ListView.Header>
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="70">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="140"></ColumnDefinition>
                                <ColumnDefinition Width="90"></ColumnDefinition>
                                <ColumnDefinition Width="90"></ColumnDefinition>
                                <ColumnDefinition Width="140"></ColumnDefinition>
                                <ColumnDefinition Width="140"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" Text="{Binding LineNumber}" FontFamily="Segoe UI Regular" TextAlignment="Center" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" TextWrapping="Wrap"></TextBlock>
                            <TextBlock Grid.Column="1" Text="{Binding LineClass}" FontFamily="Segoe UI Regular" TextAlignment="Center" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" TextWrapping="Wrap"></TextBlock>
                            <TextBlock Grid.Column="2" Text="{Binding PIDNo}" FontFamily="Segoe UI Regular" TextAlignment="Center" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" TextWrapping="Wrap"></TextBlock>
                            <TextBox Grid.Column="3" x:Name="txtOrigin" FontSize="20" FontFamily="Segoe UI Regular" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap"></TextBox>
                            <TextBox Grid.Column="4" x:Name="txtTermination" FontSize="20" FontFamily="Segoe UI Regular" VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap"></TextBox>
                        </Grid>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
            <!--<CheckBox Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Left" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" x:Name="chkSAFC" FlowDirection="RightToLeft">SEE ATTACHED FOR CONTINUATION</CheckBox>-->
        </Grid>
        <Grid Grid.Row="1" Grid.Column="4">
            <Grid.Background>
                <SolidColorBrush Color="Black" Opacity="0.5" />
            </Grid.Background>
            <Grid.RowDefinitions>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="150"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="150"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="150"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <StackPanel Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" Background="#000" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                <TextBlock FontSize="15" Foreground="#92d050" FontFamily="Segoe UI Regular" Margin="5,5,5,5" Text="NDE/PWHT chkDCOther(H)" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </StackPanel>
            <appui:UCNFCSign x:Name="nfcNPC" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="10, 5, 5, 5"></appui:UCNFCSign>
            <StackPanel Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="5" Background="#000" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                <TextBlock FontSize="15" Foreground="#92d050" FontFamily="Segoe UI Regular" Margin="5,5,5,5" Text="MEG QA NDE RELEASE(H)" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </StackPanel>
            <appui:UCNFCSign x:Name="nfcMQNR" Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="10, 5, 5, 5"></appui:UCNFCSign>
            <StackPanel Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="5" Background="#000" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                <TextBlock FontSize="15" Foreground="#92d050" FontFamily="Segoe UI Regular" Margin="5,5,5,5" Text="RELEASED FOR TEST(H)" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </StackPanel>
            <appui:UCNFCSign x:Name="nfcRFT" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="10, 5, 5, 5"></appui:UCNFCSign>
        </Grid>
        <Grid Grid.Row="1" Grid.Column="6">
            <Grid.Background>
                <SolidColorBrush Color="Black" Opacity="0.5" />
            </Grid.Background>
            <Grid.RowDefinitions>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="31"></RowDefinition>
                <RowDefinition Height="35"></RowDefinition>
                <RowDefinition Height="35"></RowDefinition>
                <RowDefinition Height="35"></RowDefinition>
                <RowDefinition Height="35"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="41"></RowDefinition>
                <RowDefinition Height="31"></RowDefinition>
                <RowDefinition Height="35"></RowDefinition>
                <RowDefinition Height="35"></RowDefinition>
                <RowDefinition Height="35"></RowDefinition>
                <RowDefinition Height="35"></RowDefinition>
                <RowDefinition Height="35"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="160"></ColumnDefinition>
                <ColumnDefinition Width="150"></ColumnDefinition>
                <ColumnDefinition Width="150"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <StackPanel Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" Background="#000" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                <TextBlock FontSize="15" Foreground="#92d050" FontFamily="Segoe UI Regular" Margin="5,5,5,5" Text="TEST INSTRUMENTS" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </StackPanel>
            <TextBlock Grid.Row="1" Grid.Column="1" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Center" Text="Gauge #1" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBlock Grid.Row="1" Grid.Column="2" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Center" Text="Gauge #2" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBlock Grid.Row="2" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="MANUFACTURER" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBox Grid.Row="2" Grid.Column="1" x:Name="txtManGauge1" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBox Grid.Row="2" Grid.Column="2" x:Name="txtManGauge2" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Top" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,2,5,0"></TextBox>
            <TextBlock Grid.Row="3" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="DESCRIPTION" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBox Grid.Row="3" Grid.Column="1" x:Name="txtDesGauge1" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBox Grid.Row="3" Grid.Column="2" x:Name="txtDesGauge2" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBlock Grid.Row="4" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="RANGE" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBox Grid.Row="4" Grid.Column="1" x:Name="txtRanGauge1" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBox Grid.Row="4" Grid.Column="2" x:Name="txtRanGauge2" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBlock Grid.Row="5" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="ID NO." FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBox Grid.Row="5" Grid.Column="1" x:Name="txtIDNoGauge1" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBox Grid.Row="5" Grid.Column="2" x:Name="txtIDNoGauge2" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBlock Grid.Row="6" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="CALIBRATION DATE" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <c1:C1DatePicker Grid.Row="6" Grid.Column="1" x:Name="txtCalibrationDtGauge1" HorizontalAlignment="Stretch"  VerticalAlignment="Center"  Xaml:C1NagScreen.Nag="True" />
            <c1:C1DatePicker Grid.Row="6" Grid.Column="2" x:Name="txtCalibrationDtGauge2" HorizontalAlignment="Stretch"  VerticalAlignment="Center"  Xaml:C1NagScreen.Nag="True" />
            <StackPanel Grid.Row="7" Grid.Column="0" Grid.ColumnSpan="5" Background="#000" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                <TextBlock FontSize="15" Foreground="#92d050" FontFamily="Segoe UI Regular" Margin="5,5,5,5" Text="TEST DATA" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </StackPanel>
            <TextBlock Grid.Row="8" Grid.Column="1" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Center" Text="START" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBlock Grid.Row="8" Grid.Column="2" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Center" Text="FINISH" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBlock Grid.Row="9" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="TIME" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBox Grid.Row="9" Grid.Column="1" x:Name="txtStartTime" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBox Grid.Row="9" Grid.Column="2" x:Name="txtFinishTime" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBlock Grid.Row="10" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="TEST PRESSURE" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBox Grid.Row="10" Grid.Column="1" x:Name="txtStartTestPressure" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBox Grid.Row="10" Grid.Column="2" x:Name="txtFinishTestPressure" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBlock Grid.Row="11" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="AMBIENT TEMP." FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBox Grid.Row="11" Grid.Column="1" x:Name="txtStartAmbient" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBox Grid.Row="11" Grid.Column="2" x:Name="txtFinishAmbient" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
            <TextBlock Grid.Row="12" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Text="PIPE TEMP." FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBlock>
            <TextBox Grid.Row="12" Grid.Column="1" x:Name="txtStartPipeTemp" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" HorizontalAlignment="Stretch" FontFamily="Segoe UI Regular" Margin="5,5,5,5"></TextBox>
