<control:LayoutAwarePage
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Element.Reveal.Manage.Discipline.ITR"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:control="using:WinAppLibrary.Controls"
    xmlns:appui="using:WinAppLibrary.UI"
    xmlns:c1="using:C1.Xaml.DateTimeEditors"
    xmlns:Xaml="using:C1.Xaml"
    x:Class="Element.Reveal.Manage.Discipline.ITR.ITR_CableTrayInspection"
    DataContext="{Binding DefaultViewModel, RelativeSource={RelativeSource Mode=Self}}"
    mc:Ignorable="d">
    <Grid x:Name="LayoutRoot">
        <Grid.Background>
            <ImageBrush ImageSource="/Assets/ITR/common_bg.jpg"
                    Stretch="UniformToFill" />
        </Grid.Background>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="120" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="154"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid x:Name="grBackButton" Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="0,0,0,0">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="120"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <TextBlock x:Name="tbpageTitle" Grid.Column="1" Grid.Row="0" Text="Field Inspection of Cable Tray" FontSize="56" Foreground="{StaticResource MediumGreen}"  FontFamily="Segoe UI Light"
                       VerticalAlignment="Top" HorizontalAlignment="Left" />
        </Grid>
        <ScrollViewer
            x:Name="ScrollViewer" 
            Grid.Row="1" Grid.Column="1"            
            AutomationProperties.AutomationId="ItemGridScrollViewer"
            Style="{StaticResource HorizontalScrollViewerStyle}"
            RenderTransformOrigin="0.5, 0.5"
            VerticalAlignment="Stretch" HorizontalAlignment="Stretch"
            ScrollViewer.HorizontalScrollMode="Enabled"
            ScrollViewer.HorizontalScrollBarVisibility="Auto"
            ScrollViewer.IsHorizontalScrollChainingEnabled="False">
            <ScrollViewer.RenderTransform>
                <TransformGroup>
                    <ScaleTransform x:Name="ScaleTransScroll" ScaleX="1" ScaleY="1" />
                    <TranslateTransform x:Name="TranTransScroll" X="0" Y="0" />
                </TransformGroup>
            </ScrollViewer.RenderTransform>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="46"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="600"/>
                    <ColumnDefinition Width="80"/>
                    <ColumnDefinition Width="700"/>
                    <ColumnDefinition Width="80"/>
                    <ColumnDefinition Width="400"/>
                    <ColumnDefinition Width="80"/>
                </Grid.ColumnDefinitions>
                <!--Project Information-->
                <StackPanel Grid.Row="0" Grid.Column="0" VerticalAlignment="Top">
                    <TextBlock TextWrapping="Wrap" FontSize="26"  Text="Project Infomation" Foreground="#fff" FontFamily="Segoe UI light"/>
                </StackPanel>
                <ScrollViewer Grid.Row="1" Grid.Column="0" VerticalScrollBarVisibility="Visible" Height="500" VerticalAlignment="Top">
                    <Grid VerticalAlignment="Top" Height="560">
                        <Grid.Background>
                            <SolidColorBrush Color="Black" Opacity="0.5" />
                        </Grid.Background>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="41"/>
                            <RowDefinition Height="41"/>
                            <RowDefinition Height="41"/>
                            <RowDefinition Height="41"/>
                            <RowDefinition Height="41"/>
                            <RowDefinition Height="41"/>
                            <RowDefinition Height="41"/>
                            <RowDefinition Height="41"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="150"/>
                            <ColumnDefinition Width="150"/>
                            <ColumnDefinition Width="150"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" Grid.Row="0" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Cabel Tray Tag No"/>
                        <TextBlock Grid.Column="0" Grid.Row="1" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Project Name"/>
                        <TextBlock Grid.Column="2" Grid.Row="1" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Job No"/>
                        <TextBlock Grid.Column="0" Grid.Row="2" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Project No"/>
                        <TextBlock Grid.Column="2" Grid.Row="2" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Contract No"/>
                        <TextBlock Grid.Column="0" Grid.Row="3" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Inspection Date"/>
                        <TextBlock Grid.Column="2" Grid.Row="3" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Inspected By"/>
                        <TextBlock Grid.Column="0" Grid.Row="4" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Ref Drwg"/>
                        <TextBlock Grid.Column="2" Grid.Row="4" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Ref Spec"/>
                        <TextBlock Grid.Column="0" Grid.Row="5" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Cable Tray From"/>
                        <TextBlock Grid.Column="2" Grid.Row="5" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Cable Tray To"/>
                        <TextBlock Grid.Column="0" Grid.Row="6" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="IWP No"/>
                        <TextBlock Grid.Column="2" Grid.Row="6" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="System No"/>
                        <TextBlock Grid.Column="0" Grid.Row="7" Margin="20,13,0,0" VerticalAlignment="Top" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="System Name"/>
                        <ComboBox x:Name="cboCableTrayTagNo"  Grid.Column="1" Grid.Row="0" Grid.ColumnSpan="3" Margin="10,0,0,0" Width="140" HorizontalAlignment="Left"/>
                        <TextBlock x:Name="txtProjectName" Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="3" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular" />
                        <TextBlock x:Name="txtJobNo" Grid.Column="3" Grid.Row="1" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular" />
                        <TextBlock x:Name="txtProjectNo" Grid.Column="1" Grid.Row="2" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"/>
                        <TextBlock x:Name="txtContractNo" Grid.Column="3" Grid.Row="2" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"/>
                        <c1:C1DatePicker x:Name="dtpInspectionDate" Grid.Column="1" Grid.Row="3" Xaml:C1NagScreen.Nag="True"/>
                        <TextBlock x:Name="txtInspectionBy" Grid.Column="3" Grid.Row="3" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"/>
                        <TextBlock x:Name="txtRefDrwg" Grid.Column="1" Grid.Row="4" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"/>
                        <TextBlock x:Name="txtRefSpec" Grid.Column="3" Grid.Row="4" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"/>
                        <TextBlock x:Name="txtCableTrayFrom" Grid.Column="1" Grid.Row="5" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"/>
                        <TextBlock x:Name="txtCableTrayTo" Grid.Column="3" Grid.Row="5" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"/>
                        <TextBlock x:Name="txtIWPNo" Grid.Column="1" Grid.Row="6" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"/>
                        <TextBlock x:Name="txtSystemNo" Grid.Column="3" Grid.Row="6" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"/>
                        <TextBlock x:Name="txtSystemName" Grid.Column="1" Grid.Row="7" Margin="10,13,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"/>
                    </Grid>
                </ScrollViewer>                
                <!--Visual Inspection and Comments-->
                <StackPanel Grid.Row="0" Grid.Column="2" VerticalAlignment="Top">
                    <TextBlock TextWrapping="Wrap" FontSize="26"  Text="Visual Inspection &amp; Comments" Foreground="#fff" FontFamily="Segoe UI light"/>
                </StackPanel>
                <Grid Grid.Row="1" Grid.Column="2" Height="500" VerticalAlignment="Top">
                    <Grid.Background>
                        <SolidColorBrush Color="Black" Opacity="0.5" />
                    </Grid.Background>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="150"/>
                    </Grid.RowDefinitions>
                    <ScrollViewer Grid.Column="0" Grid.Row="0" VerticalScrollBarVisibility="Visible" VerticalAlignment="Top">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="40"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="50"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" Grid.Row="0" Margin="10,20,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"  Text="Index"/>
                            <TextBlock Grid.Column="1" Grid.Row="0" Margin="10,20,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"  Text="N/A"/>
                            <TextBlock Grid.Column="2" Grid.Row="0" Margin="10,20,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"  Text="Yes"/>
                            <TextBlock Grid.Column="3" Grid.Row="0" Margin="10,20,0,0" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"  Text="No"/>
                            <TextBlock Grid.Column="4" Grid.Row="0" Margin="0,20,0,0" HorizontalAlignment="Center" FontSize="15" Foreground="#fff" FontFamily="Segoe UI Regular"  Text="Description"/>
                            <Image Grid.Column="0" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_01.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC1" Checked="chkVIC1_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC1Yes" GroupName="rdoVIC1"/>
                            <RadioButton Grid.Column="3" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC1No" GroupName="rdoVIC1"/>
                            <TextBlock Grid.Column="4" Grid.Row="1"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Is The Cable Tray run completed?" />
                            <Image Grid.Column="0" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_02.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC2" Checked="chkVIC2_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC2Yes" GroupName="rdoVIC2"/>
                            <RadioButton Grid.Column="3" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC2No" GroupName="rdoVIC2"/>
                            <TextBlock Grid.Column="4" Grid.Row="2"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Are cable trays the proper size and type as specified on the drawing?" />
                            <Image Grid.Column="0" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_03.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC3" Checked="chkVIC3_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC3Yes" GroupName="rdoVIC3"/>
                            <RadioButton Grid.Column="3" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC3No" GroupName="rdoVIC3"/>
                            <TextBlock Grid.Column="4" Grid.Row="3"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Are all bends and fittings the proper size and installed as per dwgs?" />
                            <Image Grid.Column="0" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_04.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC4" Checked="chkVIC4_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC4Yes" GroupName="rdoVIC4"/>
                            <RadioButton Grid.Column="3" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC4No" GroupName="rdoVIC4"/>
                            <TextBlock Grid.Column="4" Grid.Row="4"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Are all end plates and drop outs installed as per details?" />
                            <Image Grid.Column="0" Grid.Row="5" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_05.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="5" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC5" Checked="chkVIC5_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="5" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC5Yes" GroupName="rdoVIC5"/>
                            <RadioButton Grid.Column="3" Grid.Row="5" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC5No" GroupName="rdoVIC5"/>
                            <TextBlock Grid.Column="4" Grid.Row="5"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Are all expansion joints installed (c/w ground jumpers) as per details?" />
                            <Image Grid.Column="0" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_06.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC6" Checked="chkVIC6_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC6Yes" GroupName="rdoVIC6"/>
                            <RadioButton Grid.Column="3" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC6No" GroupName="rdoVIC6"/>
                            <TextBlock Grid.Column="4" Grid.Row="6"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Are trays grounded at the location shown on the dwgs?" />
                            <Image Grid.Column="0" Grid.Row="7" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_07.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="7" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC7" Checked="chkVIC7_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="7" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC7Yes" GroupName="rdoVIC7"/>
                            <RadioButton Grid.Column="3" Grid.Row="7" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC7No" GroupName="rdoVIC7"/>
                            <TextBlock Grid.Column="4" Grid.Row="7"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Are grouding conductores the proper size as per dwgs and details?" />
                            <Image Grid.Column="0" Grid.Row="8" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_08.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="8" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC8" Checked="chkVIC8_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="8" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC8Yes" GroupName="rdoVIC8"/>
                            <RadioButton Grid.Column="3" Grid.Row="8" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC8No" GroupName="rdoVIC8"/>
                            <TextBlock Grid.Column="4" Grid.Row="8"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Have all ground connections been covered with a non corrosive material?" />
                            <Image Grid.Column="0" Grid.Row="9" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_09.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="9" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC9" Checked="chkVIC9_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="9" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC9Yes" GroupName="rdoVIC9"/>
                            <RadioButton Grid.Column="3" Grid.Row="9" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC9No" GroupName="rdoVIC9"/>
                            <TextBlock Grid.Column="4" Grid.Row="9"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Is the tray cleaned of corrosive material at all joints and connections?" />
                            <Image Grid.Column="0" Grid.Row="10" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_10.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="10" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC10" Checked="chkVIC10_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="10" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC10Yes" GroupName="rdoVIC10"/>
                            <RadioButton Grid.Column="3" Grid.Row="10" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC10No" GroupName="rdoVIC10"/>
                            <TextBlock Grid.Column="4" Grid.Row="10"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Are all hold downs and sliders installed properly?" />
                            <Image Grid.Column="0" Grid.Row="11" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_11.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="11" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC11" Checked="chkVIC11_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="11" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC11Yes" GroupName="rdoVIC11"/>
                            <RadioButton Grid.Column="3" Grid.Row="11" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC11No" GroupName="rdoVIC11"/>
                            <TextBlock Grid.Column="4" Grid.Row="11"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Are all hod downs and sliders located as per site standards?" />
                            <Image Grid.Column="0" Grid.Row="12" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_12.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="12" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC12" Checked="chkVIC12_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="12" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC12Yes" GroupName="rdoVIC12"/>
                            <RadioButton Grid.Column="3" Grid.Row="12" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC12No" GroupName="rdoVIC12"/>
                            <TextBlock Grid.Column="4" Grid.Row="12"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="is the tray adequately spaced away from any hot pipes or surfaces" />
                            <Image Grid.Column="0" Grid.Row="13" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_13.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="13" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC13" Checked="chkVIC13_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="13" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC13Yes" GroupName="rdoVIC13"/>
                            <RadioButton Grid.Column="3" Grid.Row="13" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC13No" GroupName="rdoVIC13"/>
                            <TextBlock Grid.Column="4" Grid.Row="13"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Are all tray and fitting covers installed where required?" />
                            <Image Grid.Column="0" Grid.Row="14" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_14.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="14" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC14" Checked="chkVIC14_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="14" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC14Yes" GroupName="rdoVIC14"/>
                            <RadioButton Grid.Column="3" Grid.Row="14" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC14No" GroupName="rdoVIC14"/>
                            <TextBlock Grid.Column="4" Grid.Row="14"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Does the cable tray have proper identification as per site standards?" />
                            <Image Grid.Column="0" Grid.Row="15" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_15.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="15" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC15" Checked="chkVIC15_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="15" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC15Yes" GroupName="rdoVIC15"/>
                            <RadioButton Grid.Column="3" Grid.Row="15" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC15No" GroupName="rdoVIC15"/>
                            <TextBlock Grid.Column="4" Grid.Row="15"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Are all supports located as per the dwgs and site standards?" />
                            <Image Grid.Column="0" Grid.Row="16" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_16.png" Width="20" Height="20"/>
                            <CheckBox Grid.Column="1" Grid.Row="16" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="chkVIC16" Checked="chkVIC16_Checked"/>
                            <RadioButton Grid.Column="2" Grid.Row="16" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC16Yes" GroupName="rdoVIC16"/>
                            <RadioButton Grid.Column="3" Grid.Row="16" VerticalAlignment="Center" HorizontalAlignment="Center"  x:Name="rdoVIC16No" GroupName="rdoVIC16"/>
                            <TextBlock Grid.Column="4" Grid.Row="16"   Margin="10,20,0,0" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="Are all bolt heads flush with the inside rail as per detail?" />
                            <Image Grid.Column="0" Grid.Row="17" VerticalAlignment="Center" HorizontalAlignment="Center" Source="\Assets\ITR\common_bul_17.png" Width="20" Height="20"/>
