            <TextBox x:Name="txtAd" Grid.Row="5" Grid.Column="1" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Margin="5"/>
            <TextBlock Grid.Row="6" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Text="RANGE :" Margin="5"/>
            <TextBox x:Name="txtRa" Grid.Row="6" Grid.Column="1" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Margin="5"/>
            <TextBlock Grid.Row="7" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Text="SET POINT :" Margin="5"/>
