            <TextBlock Grid.Row="8" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Text="EQUIP TYPE" Margin="5"/>
            <TextBox Grid.Row="8" Grid.Column="1" x:Name="txtTEEt" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Margin="5"/>
            <TextBlock Grid.Row="9" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Text="SERIAL NO." Margin="5"/>
