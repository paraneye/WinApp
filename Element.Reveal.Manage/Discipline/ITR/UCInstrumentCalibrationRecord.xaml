        </Grid.ColumnDefinitions>
        <Grid Grid.Row="0" Grid.Column="0">
            <StackPanel VerticalAlignment="Top">
                <TextBlock TextWrapping="Wrap" FontSize="26"  Text="Project Information" Foreground="#fff" FontFamily="Segoe UI light" Margin="5"/>
