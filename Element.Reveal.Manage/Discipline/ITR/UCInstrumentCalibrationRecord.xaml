            <TextBlock Grid.Row="1" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Text="PROJECT NO." Margin="5"/>
            <TextBlock Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" x:Name="txtPno" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Margin="5"/>
            <TextBlock Grid.Row="2" Grid.Column="0" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Text="CWP / EWP NO." Margin="5"/>
            <TextBlock x:Name="txtCen" Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="3" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" VerticalAlignment="Center" FontFamily="Segoe UI Regular" Margin="5"/>
            <TextBlock Grid.Row="3" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Center" TextWrapping="Wrap" FontSize="15" Foreground="#7b91a2" FontFamily="Segoe UI Regular" Text="CONSTRACTOR JOB NO." Margin="5"/>
